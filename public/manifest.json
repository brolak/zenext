{
    "name": "Notify-Me",
    "description": "Advanced Support Ticket Notifications",
    "version": "1.0.4",
    "content_security_policy": "script-src 'self' https://www.google-analytics.com; object-src 'self'",
    "permissions": [
        "notifications",
        "https://*.zendesk.com/",
        "storage",
        "tabs"
    ],

    "icons": {
        "16": "logo_smallNEW.png",
        "48": "logo_smallNEW.png",
        "128": "logo_smallNEW.png"
    },
    "browser_action": {
        "default_popup": "index.html"
    },
    "manifest_version": 2,
    "background": {
        "scripts": [
            "axios.min.js",
            "background.js"
        ]
    }

}
